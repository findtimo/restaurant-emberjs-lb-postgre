{
  "name": "orderfood",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "orderId": {
      "type": "number",
      "required": true
    },
    "foodId": {
      "type": "number",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "order": {
      "type": "belongsTo",
      "model": "order",
      "foreignKey": "orderId"
    },
    "food": {
      "type": "belongsTo",
      "model": "food",
      "foreignKey": "foodId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
